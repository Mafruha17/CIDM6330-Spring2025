digraph {
	rankdir=TB
	Patient [label="{Patient|+Name: String\l+ID: Int\l+Devices: List\l}" shape=rectangle]
	Device [label="{Device|+Type: String\l+Data: String\l}" shape=rectangle]
	Provider [label="{Provider|+Name: String\l+Patients: List\l}" shape=rectangle]
	Admin [label="{Admin|+Role: String\l+Permissions: String\l}" shape=rectangle]
	Patient -> Device [label=Owns]
	Provider -> Patient [label=Monitors]
	Admin -> Provider [label=Manages]
}
